{
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "@node_env",
      "express_starter_db_username": "@express_starter_db_username",
      "express_starter_db_password": "@express_starter_db_password",
      "express_starter_db_host": "@express_starter_db_host",
      "express_starter_db_port": "@express_starter_db_port",
      "express_starter_db_database": "@express_starter_db_database",
      "express_starter_jwtPrivateKey": "@express_starter_jwtprivatekey",
      "express_starter_session_secret": "@express_starter_session_secret"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/node",
      "config": {
        "distDir": "build/client"
      }
    }
  ],
  "github": {
    "silent": true
  }
}