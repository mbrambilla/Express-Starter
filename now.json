{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "env": {
    "NODE_ENV": "@node_env",
    "appname_dbHost": "@appname_dbhost",
    "appname_dbName": "@appname_dbname",
    "appname_jwtPrivateKey": "@appname_jwtprivatekey"
  },
  "github": {
    "silent": true
  }
}